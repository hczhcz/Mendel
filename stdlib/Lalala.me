func bbst(array){
    var len = array.length;
    var i = 0;
    var j = 1;

    while (i < len - 1) {
        while ( j < len) {
            if ( array[j] < array[i]) {
                var x int = array[j];
                array[j] = array[i];
                array[i] = x;
                }
              j = j + 1;
            }
          i = i + 1;
        }
      
    write(array);
    }
            
