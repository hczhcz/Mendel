func bbst(lalala array){
    var len int = lalala.length();
    var i int = 0;
    var j int = 1;

    while (i < len - 1) {
        while ( j < len) {
            if ( lalala.array[j] < lalala.array[i]) {
                var x int = array[j];
                lalala.array[j] = lalala.array[i];
                lalala.array[i] = x;
                }
              j = j + 1;
            }
          i = i + 1;
        }
      
    write(lalala);
    }
            
